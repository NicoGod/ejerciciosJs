function kangaroo(x1, v1, x2, v2) {
    // Write your code here
    let jump1 = x1 + v1;
    let jump2 = x2 + v2;
  
    let div = 0;
  
    if(v1 <= v2) {
      return 'NO' 
    }
    else {
      while(x1 < x2){
          x1 += v1
          x2 += v2
      }
      if(x1 == x2) {
        console.log("YES")
      }
      else {
        console.log("NO")
      }
    }
}

kangaroo(14, 4 ,98 ,2) // porque mierda, revisar cuenta